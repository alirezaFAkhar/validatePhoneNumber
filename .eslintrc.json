{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["eslint-plugin-no-only-tests", "prettier"],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-prototype-builtins": "off",
    "object-curly-spacing": "off",
    "no-only-tests/no-only-tests": "error",
  },
  "overrides": []
}
